#LSP.INJECTOR inj_mdot GEOM POINT X -0.102 0.02 0.0375 V 100 0 0 TYPE COUNT 1 DIST UNIFORM 2e-5 TP 320
#LSP.INJECTOR inj_mdot_rand GEOM BOX X -0.052 -0.052 -0.0185 0.0355 0.02 0.045 V 100 0 0 TYPE MDOT 2e-5 DIST RR 3.5e-6 50e-6 29e-6 2 1 TP $(this_Tref)
#LSP.INJECTOR inj_mdot_small GEOM BOX X -0.052 -0.052 -0.0198 0.0368 0.0191 0.047 V 100 0 0 TYPE MDOT 5e-7 DIST UNIFORM 3e-6 TP $(this_Tref)
#LSP.INJECTOR inj_mdot_medium GEOM BOX X -0.052 -0.052 -0.0185 0.0355 0.02 0.045 V 100 0 0 TYPE MDOT 1e-5 DIST UNIFORM 10e-6 TP $(this_Tref)
LSP.INJECTOR inj_mdot_large GEOM BOX X -0.052 -0.052 -0.0198 0.0368 0.0191 0.047 V 100 0 0 TYPE MDOT 2e-4 DIST UNIFORM 30e-6 TP $(this_Tref)
#LSP.INJECTOR inj_mdot_rand GEOM BOX X -0.052 -0.052 -0.017 0.033 0.0075 0.0175 V 100 0 0 TYPE MDOT 2e-5 DIST RR 3.5e-6 50e-6 29e-6 2 1 TP $(this_Tref)

