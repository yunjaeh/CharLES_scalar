#
VERBOSE

DEFINE check = 20
DEFINE this_gamma 1.4
DEFINE this_pref 1e5
DEFINE Ugx 200.0
DEFINE rhog 0.18 
DEFINE this_Tref 2000
DEFINE this_Twall 1300

RESTART ../stitch/vki.50k.mles
#RESTART=/home/miladm/SAND_Lynn/vki/stitch2/restart_Nz10_100k_new.mles result_ig.00010000.sles
#RESTART=/home/miladm/SAND_Lynn/vki/stitch2/restart_Nz10_100k_new.mles result_3um.00020000.sles
#RESTART=/home/miladm/SAND_Lynn/vki/stitch2/restart_Nz10_100k_new.mles result_10um.00020000.sles
#RESTART=/home/miladm/SAND_Lynn/vki/stitch2/restart_Nz10_100k_new.mles result_30um.00020000.sles
#RESTART=/home/miladm/SAND_Lynn/vki/stitch2/restart_Nz10_100k.mles

# Initial condition specification:
INIT_RUP $(rhog) $(Ugx) 0 0 $(this_pref)

CHECK_INTERVAL=$(check)
#NSTEPS = 10

TIMESTEP CFL=1

# specification of the sub-gridscale model:
SGS_MODEL=VREMAN

# Specification of gas properties:
# the following parameters require specification
EOS = IDEAL_GAS_LSP
#EOS = IDEAL_GAS
P_REF $(this_pref)
RHO_REF $(rhog)
T_REF $(this_Tref)
MU_REF 1.8E-5

#inlet CBC_TOTAL_PT 2.2e5 2000 0.1 # what is appropriate t_relax?
inlet INFLOW_TURB PROFILE CONSTANT_UTKETP $(Ugx) 0 0 20 $(this_Tref) $(this_pref) L 0.05 UN $(Ugx)
outlet NSCBC_OUTLET_P 0.8e5 0.15 0.4 #101325 0.15 0.4 
wall_blade WALL_ISOTHERMAL $(this_Twall)
wall_blade_te WALL_ISOTHERMAL $(this_Twall)
bars WALL_ISOTHERMAL $(this_Twall)

# ====================
# lsp settings
# ====================
LSP.TIME_INT 2WAY_COUPLED
LSP.INIT NONE
#LSP.INIT RANDOM 7e-4
#LSP.AGGLEM 10000 1000
LSP.MATERIAL SOLID DUST
#LSP.MATERIAL LIQUID HEXANE_MHB98
LSP.PLANE_TRASH exit1 X 0.1 -0.1436 0.0321 N 1 0 0 R 0.1
DP_MAG = 400
#DP_MAG = 130
#DP_MAG = 40


# ===================
# Breakup model
# ===================
BREAKUP_MODEL NO_BREAKUP
#BREAKUP_MODEL SBM WEBER_CRITICAL 6

# =================
# injector setting
