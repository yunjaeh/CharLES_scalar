#INTERP_FROM_RESTART ../stitch/restart.root3.mles result.00000100.sles TRANSLATE 1 1 1
#POST
#SNAPSHOT NAME=particles RANGE=1 1 3
#SNAPSHOT NAME=result RANGE=1 1 3
